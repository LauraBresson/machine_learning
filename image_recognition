    "# setting the layers from the blueprint to be static\n",
    "for layer in model.layers[:87]:\n",
    "    layer.trainable= False\n",
    "    \n",
    "# setting my own layers to be trainable\n",
    "for layer in model.layers[87:]:\n",
    "    layer.trainable= True"
   ]
